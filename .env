# .env.example
# Скопируйте этот файл в .env и при необходимости измените значения.

# URL для подключения к Postgres (локально)
DATABASE_URL=postgresql://antitelegram_user:password@localhost:5432/antitelegram_db

# Секрет для JWT (обязательно заменить в продакшне)
SECRET_KEY=change_this_secret_in_prod

# Время жизни токена в минутах
ACCESS_TOKEN_EXPIRE_MINUTES=10080
